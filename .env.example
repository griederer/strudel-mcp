# Strudel MCP Configuration

# Strudel REPL URL (default: https://strudel.cc)
STRUDEL_URL=https://strudel.cc

# Browser headless mode (true/false)
# Set to false for debugging or if audio doesn't work
HEADLESS=true

# Browser window size
WINDOW_WIDTH=1280
WINDOW_HEIGHT=720

# Default BPM
DEFAULT_BPM=120

# Pattern save directory (relative to project root)
PATTERNS_DIR=./patterns

# Debug mode (true/false)
DEBUG=false
